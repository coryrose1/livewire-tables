{
    "name": "coryrose/livewire-tables",
    "description": "An extension for Livewire that allows you to effortlessly scaffold datatables with optional pagination, search, and sort.",
    "license": "MIT",
    "authors": [
        {
            "name": "Cory Rosenwald",
            "email": "coryrosenwald@gmail.com",
            "homepage": "https://coryrose.dev"
        }
    ],
    "homepage": "https://github.com/coryrose1/livewire-tables",
    "keywords": ["Laravel", "Livewire", "livewire-tables"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "livewire/livewire": "^0.3.17"
    },
    "autoload": {
        "psr-4": {
            "Coryrose\\LivewireTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coryrose\\LivewireTables\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coryrose\\LivewireTables\\LivewireTablesServiceProvider"
            ]
        }
    }
}
